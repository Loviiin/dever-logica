def comeco():
    global entrada
    print("seja bem vindo ao cinema")
    entrada=(input("seu filme começou?"))


def horario():
    global minutos
    if entrada == "sim":
        minutos = int(input("a quanto tempo seu filme começou?"))
        if minutos <= 30:
            print("pode entrar")
        else:
            print("atrasado")
    if entrada == "nao":
        minutos = int(input("seu filme começa em quantos minutos?"))
        if minutos > 30:
            print("muito cedo")
        else:
            print("pode entrar")

def main():
    comeco()
    horario()
main()
